# Tracy debugger
#dbg: TRUE
dbg: FALSE

# CORE
app: app
canonical_url: 'https://app.gscloud.cz/'
date_default_timezone: 'Europe/Prague'
project: TESSERACT
secret_cookie_key: secure.key
version: v1

# Google OAUTH 2.0
goauth_origin: 'https://app.gscloud.cz'
goauth_redirect: 'https://app.gscloud.cz/login'

# local Google OAUTH 2.0
local_goauth_origin: 'http://localhost:8014'
local_goauth_redirect: 'http://localhost:8014/login'

# cache profiles
cache_profiles: [
  tenminutes: "+10 minutes"
]

# Redis server configuration
redis: [
  port: 6377
  timeout: 1
]

# extra data
data: [
]

# locales
